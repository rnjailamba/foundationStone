# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET           /userService/ping                                   com.cementify.userservice.controllers.CustomerController.ping()
POST          /userService/create                                 com.cementify.userservice.controllers.CustomerController.create()
GET           /userService/findByMobile/:mobile                   com.cementify.userservice.controllers.CustomerController.findByMobile(mobile: String)
POST          /userService/login                                  com.cementify.userservice.controllers.CustomerController.login()
GET           /userService/findCustomerById/:customerId           com.cementify.userservice.controllers.CustomerController.findCustomerByCustomerId(customerId: Integer)
GET           /userService/findCustomerByEmail/:email             com.cementify.userservice.controllers.CustomerController.findCustomerByEmail(email: String)
GET           /userService/findCustomerByFbId/:fbId               com.cementify.userservice.controllers.CustomerController.findCustomerByFbId(fbId: String)
POST          /userService/updatePassword                         com.cementify.userservice.controllers.CustomerController.updatePassword()
POST          /userService/updateFbId                             com.cementify.userservice.controllers.CustomerController.updateFbId()
POST          /userService/updateGoogleId                         com.cementify.userservice.controllers.CustomerController.updateGoogleId()
POST          /userService/setVerified                            com.cementify.userservice.controllers.CustomerController.setVerified()
POST          /userService/logout                                 com.cementify.userservice.controllers.CustomerController.logout()
GET           /userService/findCustomerByRuid/:ruid               com.cementify.userservice.controllers.CustomerController.findCustomerByRuid(ruid: String)
POST          /userService/updateCustomerData                     com.cementify.userservice.controllers.CustomerController.updateCustomerData()
GET           /userService/getCustomerContacts/:customerId        com.cementify.userservice.controllers.CustomerController.getCustomerContacts(customerId: Integer)
GET           /userService/getCustomerLocations/:customerId       com.cementify.userservice.controllers.CustomerController.getCustomerLocations(customerId: Integer)
GET           /userService/getCustomerEmails/:customerId          com.cementify.userservice.controllers.CustomerController.getCustomerEmails(customerId: Integer)
GET           /userService/getCustomerAddress/:customerId         com.cementify.userservice.controllers.CustomerController.getCustomerAddress(customerId: Integer)
POST          /userService/addCustomerContact                     com.cementify.userservice.controllers.CustomerController.addCustomerContact()
POST          /userService/addCustomerLocation                    com.cementify.userservice.controllers.CustomerController.addCustomerLocation()
POST          /userService/addCustomerAddress                     com.cementify.userservice.controllers.CustomerController.addCustomerAddress()
POST          /userService/addCustomerEmail                       com.cementify.userservice.controllers.CustomerController.addCustomerEmail()
POST          /userService/addCustomerContactList                 com.cementify.userservice.controllers.CustomerController.addCustomerContactList()
POST          /userService/addCustomerLocationList                com.cementify.userservice.controllers.CustomerController.addCustomerLocationList()
POST          /userService/addCustomerAddressList                 com.cementify.userservice.controllers.CustomerController.addCustomerAddressList()
POST          /userService/addCustomerEmailList                   com.cementify.userservice.controllers.CustomerController.addCustomerEmailList()
POST          /userService/resetPassword                          com.cementify.userservice.controllers.CustomerController.resetPassword()
POST          /userService/createCustomer                         com.cementify.userservice.controllers.CustomerController.createCustomer()

